@model IEnumerable<DogShelterMvc.Models.Log>

@{
    ViewData["Title"] = "Logy";
}

<h1>Logy</h1>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Id)</th>
            <th>@Html.DisplayNameFor(model => model.CUser)</th>
            <th>@Html.DisplayNameFor(model => model.EventTime)</th>
            <th>@Html.DisplayNameFor(model => model.TableName)</th>
            <th>@Html.DisplayNameFor(model => model.Operation)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Id)</td>
            <td>@Html.DisplayFor(modelItem => item.CUser)</td>
            <td>@Html.DisplayFor(modelItem => item.EventTime)</td>
            <td>@Html.DisplayFor(modelItem => item.TableName)</td>
            <td>@Html.DisplayFor(modelItem => item.Operation)</td>
            <td>
                <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-sm btn-info">Detail</a>
            </td>
        </tr>
}
    </tbody>
</table>

